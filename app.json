{
  "expo": {
    "name": "minecraft-app",
    "slug": "minecraft-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/minecraft.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/minecraft.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "navigation": {
      "router": "expo-router"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rifan.alshahrani.minecraftapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/minecraft.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rifan.alshahrani.minecraftapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
